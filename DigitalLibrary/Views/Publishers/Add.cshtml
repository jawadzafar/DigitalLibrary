@model  DigitalLibrary.Models.Publisher

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminDashboardPartial.cshtml";
}

<h2>Create</h2>


@using (Html.BeginForm("Add", "Publishers", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    @Html.AntiForgeryToken()
    <div class="row" >
        <div class="form-horizontal"  style="width:600px; margin:0 auto;">
            <h4>Publisher </h4>
            <hr />
            <div class="form-group">
                <label name="" class="col-md-2 form-control">Name</label>
                <div class="col-md-10">
                    <input name="Name" type="text" class="form-control" />
                </div>
            </div>
          
            <div class="form-group">
                <label class="col-md-2 form-control"> Publisher Address</label>
                <div class="col-md-10">
                    <input name="Address" type="text" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
    </div>
}

<div>

</div>