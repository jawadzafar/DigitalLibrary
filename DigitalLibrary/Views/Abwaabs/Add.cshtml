@model  DigitalLibrary.Models.Abwaab

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm("Add", "Abwaabs", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    @Html.AntiForgeryToken()
    <div class="row">
        <div class="form-horizontal">
            <h4>Abwaabs</h4>
            <hr />
            <div class="form-group">
                <label name="" class="col-md-2 form-control">Name</label>
                <div class="col-md-10">
                    <input name="Name" type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-2 form-control"> Baab No</label>
                <div class="col-md-10">
                    <input name="BaabNo" type="text" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-2 form-control"> No Of Pages</label>
                <div class="col-md-10">
                    <input name="NoOfPages" type="text" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-2 form-control"> Book Name</label>
                <div class="col-md-10">
                    <input name="BookId" type="text" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
    </div>
}

<div>

</div>