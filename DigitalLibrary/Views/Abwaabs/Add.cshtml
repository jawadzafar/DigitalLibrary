@model  DigitalLibrary.Models.Abwaab


@using DigitalLibrary.Models
@{
    Users user = Session["User"] as Users;
    if (user.Role == "admin")
    {
        Layout = "~/Views/Shared/_AdminDashboardPartial.cshtml";
    }
    else if (user.Role == "manager")
    {
        Layout = "~/Views/Shared/_ManagerDashoardPartial.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}
<section class="content">
    <h2>Create</h2>


    @using (Html.BeginForm("Add", "Abwaabs", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {

        @Html.AntiForgeryToken()
        <div class="row">
            <div class="form-horizontal" style="width:800px; margin:0 auto;">
                <h4>Abwaabs</h4>
                <hr />
                <div class="form-group">
                    <label name="" class="col-md-2 form-control">Name</label>
                    <div class="col-md-10">
                        <input name="Name" type="text" class="form-control" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-2 form-control"> Baab No</label>
                    <div class="col-md-10">
                        <input name="BaabNo" type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-2 form-control"> No Of Pages</label>
                    <div class="col-md-10">
                        <input name="NoOfPages" type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-2 form-control"> Book Name</label>
                    <div class="col-md-10">
                        <input name="BookId" type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
            </div>
        </div>
    }

    <div>

    </div>
</section>